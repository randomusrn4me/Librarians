<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="350.0" prefWidth="445.0" xmlns="http://javafx.com/javafx/11.0.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFXMLCTRL">
    <children>
        <JFXButton fx:id="loginButton" layoutY="300.0" onAction="#handleLoginButtonPushed" prefHeight="50.0" prefWidth="225.0" stylesheets="@addbook.css" text="Login" />
        <JFXTextField fx:id="usernameBar" alignment="TOP_LEFT" focusColor="#2f5ad0" labelFloat="true" layoutX="4.0" layoutY="20.0" prefHeight="40.0" prefWidth="450.0" promptText="Username" unFocusColor="#5e751c">
            <padding>
                <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
            </padding>
        </JFXTextField>
        <JFXTextField fx:id="PasswordBar" alignment="TOP_LEFT" focusColor="#2f5ad0" labelFloat="true" layoutX="4.0" layoutY="79.0" prefHeight="40.0" prefWidth="450.0" promptText="Password" unFocusColor="#5e751c">
            <padding>
                <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
            </padding>
        </JFXTextField>
    </children>
</AnchorPane>
